{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 30,
      "memory": 3008,
      "bodyParser": {
        "sizeLimit": "25mb"
      }
    }
  }
}